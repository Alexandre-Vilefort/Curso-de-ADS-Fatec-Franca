Algoritmo "exercicio32"


Var

   i,num: inteiro

Inicio
   //Entradas
   escreva("Digite um número: ")
   leia(num)
   escreval
   
   //Processamento
   escreval("Os divisores de",num," são:")
   escreval()
   para i de 1 até num faca
        se num % i = 0 entao
           escreval(i)
        senao
        
        fimse
   fimpara

   
   //Saídas

Fimalgoritmo