Algoritmo "exercicio10"

Var

   grausF, grausC: real

Inicio

      //entrada
      escreval("Quantos graus Farenheit?")
      leia(grausF)
      escreval()
      
      //processamento
      grausC <- 5/9*(grausF - 32)
      
      
      //saída
      escreval("O valor em graus Celsius é de: ", grausC:4:2)
      
      

Fimalgoritmo